   "source": [
    "from collections import deque\n",
    "\n",
    "def tail(path, maxlen=5):\n",
    "    result = None\n",
    "    with open(path, \"r\") as f:\n",
    "        result = deque(f, maxlen)\n",
    "    yield from result\n",
    "\n",
    "for el in tail(\"test.xml\"):\n",
    "    print(el)"
   ]
  }
